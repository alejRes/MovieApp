doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        button#btCrear Crear pelicula nueva
            a(href="/createMovie") 
        if(data)    
            ul
                each val, index in data
                    div
                        li= key + ': ' + val
                        img#edit[index](src="./images/edit.png", alt="edit")
                        img#delete[index](src="./images/delete.png", alt="edit")
        else    
            p Datos no encontrados